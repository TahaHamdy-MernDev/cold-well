<% if (success_msg.length > 0) { %>
    <div class="alert alert-success" role="alert">
        <%= success_msg %>
    </div>
<% } %>
<% if (error_msg.length > 0) { %>
    <div class="alert alert-danger" role="alert">
        <%= error_msg %>
    </div>
<% } %>
<% if (error.length > 0) { %>
    <div class="alert alert-danger" role="alert">
        <%= error %>
    </div>
<% } %>